#compdef print_completion

# zsh completion for print_completion
# long options only

_arguments -S -s \
  '(__FMT)--mp4[Prefer mp4-friendly formats/containers when possible]' \
  '(__FMT)--webm[Prefer webm-friendly formats/containers when possible]' \
  '--quiet[Suppress "Executing:" message]' \
  '--dry-run[Show yt-dlp command without executing]' \
  '--show-filename[Print output filename without downloading]' \
  '--completion[Print this zsh completion script and exit]' \
  '--help[Show help]' \
  '1:mode:( \
    music-video \
    audio-only \
    video-only \
    adult \
    youtube \
    best-quality \
  )' \
  '2:URL:_urls' \
  '*::yt-dlp args:_default'
