<?xml version="1.0" encoding="UTF-8"?>
<page name="TRANSPORT">
  <pad1 name="◀ BAR" color="color '#FF2600'" autodim="false">goto -4</pad1>
  <pad2 name="BAR  ▶" color="color '#FF2600'" autodim="false">goto +4</pad2>
  <pad3 name="◀ BEAT" color="color '#FFFF44'" autodim="false">goto -1</pad3>
  <pad4 name="BEAT ▶" color="color '#FFFF44'" autodim="false">goto +1</pad4>
  <!-- <pad5 name="BPM SRC" color="color '#8534FF'" autodim="false">var '@$abpmt_source' 1 ? auto_bpm_transition 'source_original' : auto_bpm_transition</pad5> -->
  <!-- <pad12 name="`filter_label 'name' &amp; param_uppercase`" color="color magenta" query="effect_active 'colorfx'">effect_active 'colorfx'</pad12> -->
  <!-- <pad5 name="`leftdeck ? get_text '◀SWAP' : get_text 'SWAP ▶'`" color="color white" autodim="false">rightdeck ? deck left volume 0% &amp; deck right volume 100% &amp; deck right play : deck right volume 0% &amp; deck left volume 100% &amp; deck left play</pad5> -->
  <pad5 name="ACAPELLA" color="color '#35FF29'">stem_pad 'acapella'</pad5>
  <pad6 name="INSTRUMENTAL" color="color '#CC55FF'">stem_pad 'instrumental'</pad6>
  <pad7 name="`effect_stems &amp; param_uppercase`" color="color 'yellow'">effect_stems +1</pad7>
  <pad8 name="COLORFX" color="color magenta" query="effect_active 'colorfx'">effect_active 'colorfx'</pad8>
  <pad9 name="KEY" color="masterdeck ? color orange : match_key ? color green : color red" autodim="true" query="not masterdeck ? match_key ? true : blink 500ms">match_key</pad9>
  <pad10 name="BEAT" color="masterdeck ? color orange : is_sync ? color green : color red" autodim="true" query="not masterdeck ? is_sync ? true : blink 500ms">sync</pad10>
  <pad11 name="PHRASE" color="masterdeck ? color orange : is_sync ? color green : color red" autodim="true" query="not masterdeck ? is_sync ? true : blink 500ms">sync &amp; phrase_sync 'auto'</pad11>
  <pad12 name="`masterdeck ? masterdeck_auto ? get_text 'MASTER [AUTO]' : get_text 'MASTER' : get_text 'SLAVE'`" query="masterdeck ? masterdeck_auto ? blink 2bt : true" color="masterdeck ? color orange : color white">masterdeck ? masterdeck_auto : masterdeck</pad12>
  <pad13 name="BPM －" color="color '#75D5FF'" autodim="false">pitch -1 bpm</pad13>
  <pad14 name="BPM ＋" color="color '#75D5FF'" autodim="false">pitch +1 bpm</pad14>
  <pad15 name="BPM RESET" color="blue" autodim="false">pitch_reset 4bt</pad15>
  <pad16 name="`play ? get_text 'PAUSE' : get_text '▶ PLAY'`" color="color &quot;green&quot;">play_button</pad16>
  <shift_pad16 name="STOP" color="color &quot;red&quot;" query="true">pause_stop</shift_pad16>
  <param1 name="BPM `get_bpm`">param_bigger 0 ? pitch +1 bpm : pitch -1 bpm</param1>
  <param2 name="KEY `get_key`">param_bigger 0 ? key_move +1 : key_move -1</param2>
</page>
